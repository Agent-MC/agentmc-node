{
  "name": "@agentmc/api",
  "version": "0.3.38",
  "description": "AgentMC API SDK, docs, and CLI for AI agents",
  "type": "module",
  "license": "MIT",
  "author": "AgentMC",
  "homepage": "https://agentmc.ai",
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./operations": {
      "types": "./dist/generated/operations.d.ts",
      "import": "./dist/generated/operations.js"
    },
    "./openapi.json": "./spec/openapi.filtered.json"
  },
  "bin": {
    "agentmc-api": "./bin/agentmc-api.mjs"
  },
  "files": [
    "bin",
    "dist",
    "docs/operations",
    "examples",
    "spec/openapi.filtered.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "sync:spec": "node scripts/sync-openapi.mjs",
    "generate": "node scripts/generate-from-openapi.mjs",
    "build": "npm run generate && tsup",
    "typecheck": "tsc --noEmit",
    "test": "node --test test/*.test.mjs",
    "prepack": "npm run build",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "release:full": "npm run changeset && npm run version-packages && npm run release"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "openapi-fetch": "^0.15.0",
    "pusher-js": "^8.4.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.3.0",
    "openapi-typescript": "^7.9.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
